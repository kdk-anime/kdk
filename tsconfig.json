{
	"compilerOptions": {
		"baseUrl": "src",
		"module": "commonjs",
		"lib": [ "es2021", "es6", "dom", "webworker" ],
		"target": "es2017",
		"sourceMap": true,
		"allowJs": true,
		"moduleResolution": "Node",
		"rootDir": "src",
		"jsx": "react",
		"jsxFactory": "TSXe.createElement",
		"jsxFragmentFactory": "TSXe.Fragment",
		"paths": {
			"@kdk/kdk-anime/App/*": ["App/*"]
		}
	},
	"include": [
		"src"
	],
	"exclude": [
		"node_modules", "dist"
	]
}
